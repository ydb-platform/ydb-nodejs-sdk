{
  "name": "ydb-sdk",
  "version": "5.1.1",
  "description": "Node.js bindings for working with YDB API over gRPC",
  "main": "build/cjs/src/index.js",
  "module": "build/esm/src/index.js",
  "exports": {
    ".": {
      "import": "./build/esm/src/index.js",
      "require": "./build/cjs/src/index.js"
    }
  },
  "files": [
    "build/**"
  ],
  "scripts": {
    "test:unit": "exit 0",
    "test:integration:development": "cross-env TEST_ENVIRONMENT=dev jest --config jest.config.development.js",
    "test:integration:production": "jest --config jest.config.production.js",
    "test:all": "run-p test:unit test:integration:production",
    "test": "npm run test:unit",
    "build": "tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf build",
    "prepublish": "npm run clean && npm run build && node ./fixup.js"
  },
  "keywords": [
    "ydb",
    "sdk",
    "grpc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ydb-platform/ydb-nodejs-sdk.git"
  },
  "author": "Timur Sufiev",
  "license": "Apache",
  "dependencies": {
    "@grpc/grpc-js": "^1.5.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "long": "^4.0.0",
    "luxon": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "ydb-sdk-proto": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.144",
    "@types/luxon": "^1.21.0",
    "@types/node": "^12.20.55",
    "@types/uuid": "^8.3.4",
    "@yandex-cloud/nodejs-sdk": "^2.0.0",
    "cross-env": "^7.0.3",
    "husky": "^7.0.4",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.6.4"
  },
  "peerDependenciesMeta": {
    "@yandex-cloud/nodejs-sdk": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@yandex-cloud/nodejs-sdk": "^2.0.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
